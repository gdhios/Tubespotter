{
  "name": "Tubespotter",
  "version": "1.0",
  "description": "Turn a YouTube video into a Spotify track with one click!",
  "icons": {
    "16": "icon16.png",
	"48": "icon48.png",
	"128": "icon128.png"
  },
  "options_page": "settings.html",
  "browser_action": {
    "default_icon": "icon19.png",
	"popup": "tubespotter.html"
  },
  "permissions": [
	"tabs",
    "http://ws.spotify.com/",
	"http://www.youtube.com/",
	"http://youtube.com/",
	"http://ax.phobos.apple.com.edgesuite.net/"
  ]
}